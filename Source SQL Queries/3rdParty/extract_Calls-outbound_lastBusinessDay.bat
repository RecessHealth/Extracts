bcp "select [filename], CUBSID, client_id, agentid, campaignID, termcode, lastcalldate, (case when AgentID not in ('') then 1 else 0 end) as outbound, (case when termcode in ('03','','16','13','14','15','17','48','52','PP','VS') then 1 when termcode in ('','01','02','BR','CF','FM','FX','NR','ON','OV','PG','UK') then null else 0 end) as pickup, (case when termcode in ('13','14','15','17','48','52','PP','VS') then 1 else 0 end) as phonepay from vw_dialerstatsnew where agentid in ('BRS','BR6','BRB') and lastcalldate = case when datepart(weekday, getutcdate()) in (3,4,5,6) then convert(datetime,datediff(day,0,getutcdate()-1),102) when datepart(weekday, getutcdate()) in (2) then convert(datetime,datediff(day,0,getutcdate()-3),102) end;" queryout %1\Calls-outbound_%2.txt -S "pfs-sql1" -d "PFS" -U readonly -P today05 -c